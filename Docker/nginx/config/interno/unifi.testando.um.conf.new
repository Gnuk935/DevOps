server {
    listen 443 ssl default_server;
    listen [::]:443 ssl default_server;

    server_name unifi.testando.um;

    location /wss/ {
        proxy_pass https://10.5.9.3:8443/;
        proxy_read_timeout 86400;
        include /etc/nginx/padProxy.conf;
        include /etc/nginx/webProxy.conf;
    }

    location / {
        proxy_pass https://10.5.9.3:8443/;
        include /etc/nginx/padProxy.conf;
    }

}